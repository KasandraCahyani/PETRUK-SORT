#include <iostream>
using namespace std;

int main(){

int data[100];
int terkecil, x, i ,j;

cin>>x;

for(int i=0; i<x; i++){
  cin>>data[i];
}

for(int i=x; i>=0; i--){ 
  for(int j=0; j<i; j++){
      if(data[j]>data[j+1]){
          swap(data[j],data[j+1]);
       }
  }
}

for(int i=0; i<x; i++){
  cout<<data[i]<<' '<<endl;
}

}
