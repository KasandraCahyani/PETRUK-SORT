#include <iostream>
using namespace std;

//Section Sort




//Bubble Sort
void buble_sort (int data[], int n){
int temp;
	for(int i=(n-1); i>=1; i--)
  {
		for(int j=1; j<=i; j++)
    {
			if(data[j]>data[j+1])
      {
				temp=data[j+1];
				data[j+1]=data[j];
				data[j]=temp;
			}
		}
	}
}




//Marge Sort
