#include <iostream>
using namespace std;

int main(){

int data[100];
int terkecil, n;

cout<<"Masukkan angka : ";
for(int i=0;i<n;i++){
    cin>>data[i];
} cout<<endl;

for(int i=0;i<n;i++){
    terkecil=i;
    for(int j=i;j<n;j++){
        if (data[j]<data[terkecil]){
            terkecil=j;
        }
    }
    swap(data[i],data[terkecil]);
}
for (int i=0;i<n;i++){
    cout<<data[i]<<' ';
}

}
