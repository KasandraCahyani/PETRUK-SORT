#include <iostream>
using namespace std;

int main () {
	int data[100];
	int terkecil, x;
	
	cout<<"Masukkan jumlah data : ";
	cin>>x;
	
	for (int i=0;i<x;i++) {
		cin>>data[i];
	}
	
	for (int i=0;i<x;i++) {
		terkecil=i;
		for (int j=1;j<x;j++) {
			if (data[j] < data[terkecil]) {
				terkecil=j;
			}
		} swap(data[i],data[terkecil]);
	}
	
	for (int i=0;i<x;i++) {
		cout<<data[i]<<" ";
	}
	
	return 0;
}
